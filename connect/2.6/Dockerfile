ENV DEBEZIUM_VERSION="2.6.2.Final"
ENV MAVEN_REPO_CENTRAL=""
ENV MAVEN_REPOS_ADDITIONAL=""
ENV MONGODB_MD5=bbcbbf8880be252249b15b2f0a75c950
ENV MYSQL_MD5=f2f8f8a0fbd3d84af6ee3f6aab5b0a54
ENV POSTGRES_MD5=5e5c428927179fd6df6d727ba95e9e1c
ENV SQLSERVER_MD5=e5cea53628fd46d35f525d75ac410388
ENV ORACLE_MD5=de3ba5fe660055c595012ea2cb153c28
ENV DB2_MD5=1f86d380947c31b840290ff19685bb27
ENV SPANNER_MD5=b6461bf69a1835411ec8f26639f1e52d
ENV VITESS_MD5=1a21bdfd27c9edaae7493344d5299f8f
ENV JDBC_MD5=ba9f5cea0c7a8dc925c4035b11ddeb71
ENV INFORMIX_MD5=3903b870a97a2ea161b7834d857e6b94
ENV IBMI_MD5=3bf900580328079c97f9e430c697b469
ENV KCRESTEXT_MD5=f291987d8df27b8019a3f7fe81f953c0
ENV SCRIPTING_MD5=7104dc0f933741c586e26ac62f9b948e

RUN docker-maven-download debezium mysql "$DEBEZIUM_VERSION" "$MYSQL_MD5"
RUN docker-maven-download debezium postgres "$DEBEZIUM_VERSION" "$POSTGRES_MD5"
